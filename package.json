{
  "name": "mern-services-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "proxy": {
    "/auth/google": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "engines": {
    "node": "12.20.1",
    "npm": "6.14.10"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch dist --exec \"node ./index.js\"",
    "dev:build-client": "npm run build:client-dev",
    "build:client-dev": "webpack --config webpack.client.js --watch",
    "build:server-dev": "webpack --config webpack.server.js --watch",
    "start": "node ./index.js",
    "build:client": "webpack --config webpack.client.prod.js",
    "build:server": "webpack --config webpack.server.prod.js",
    "build": "webpack --config webpack.client.prod.js"
  },
  "author": "Tanwinder Sidhu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tanwinder/MERN-services-UI/issues"
  },
  "homepage": "https://github.com/Tanwinder/MERN-services-UI#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "dotenv-webpack": "^6.0.0",
    "html-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-merge": "^5.4.0",
    "webpack-nano": "^1.1.1",
    "webpack-node-externals": "^2.5.2"
  }
}
